<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.ensighten.qatest</groupId>
  <artifactId>gtest</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>Tag Tester</name>
  <description>Tag Tester</description>
  <dependencies>
  	<dependency>
  		<groupId>org.testng</groupId>
  		<artifactId>testng</artifactId>
  		<version>6.8.8</version>
  	</dependency>
  	<dependency>
  		<groupId>net.lightbody.bmp</groupId>
  		<artifactId>browsermob-proxy</artifactId>
  		<version>2.0-beta-9</version>
  	</dependency>
  	<dependency>
  		<groupId>com.googlecode.json-simple</groupId>
  		<artifactId>json-simple</artifactId>
  		<version>1.1.1</version>
  	</dependency>
  	<dependency>
  		<groupId>org.apache.maven.plugins</groupId>
  		<artifactId>maven-surefire-plugin</artifactId>
  		<version>2.17</version>
  		<type>maven-plugin</type>
  	</dependency>
  	<dependency>
    <groupId>harlib</groupId>
    <artifactId>harhar</artifactId>
    <version>1.1.1</version>
    <scope>system</scope>
    <systemPath>${project.basedir}/lib/harlib-1.1.1.jar</systemPath>
</dependency>
<dependency>
    <groupId>bcr</groupId>
    <artifactId>bcr</artifactId>
    <version>2.0.0</version>
    <scope>system</scope>
    <systemPath>${project.basedir}/lib/bcr-2.0.2.jar</systemPath>
</dependency>
<dependency>
    <groupId>jackson</groupId>
    <artifactId>jackson</artifactId>
    <version>2.2.3</version>
    <scope>system</scope>
    <systemPath>${project.basedir}/lib/jackson-core-2.2.3.jar</systemPath>
</dependency>
<dependency>
    <groupId>jetty</groupId>
    <artifactId>jetty</artifactId>
    <version>1.1.1</version>
    <scope>system</scope>
    <systemPath>${project.basedir}/lib/jetty-util-7.3.0.v20110203.jar</systemPath>
</dependency>
<dependency>
    <groupId>json</groupId>
    <artifactId>json</artifactId>
    <version>1.1.1</version>
    <scope>system</scope>
    <systemPath>${project.basedir}/lib/json-simple-1.1.1.jar</systemPath>
</dependency>
<dependency>
    <groupId>selstandalone</groupId>
    <artifactId>selstandalone</artifactId>
    <version>1.1.1</version>
    <scope>system</scope>
    <systemPath>${project.basedir}/lib/selenium-server-standalone-2.32.0.jar</systemPath>
</dependency>
  </dependencies>
  <build>
  	<plugins>
  		<plugin>
  			<groupId>org.apache.maven.plugins</groupId>
  			<artifactId>maven-surefire-plugin</artifactId>
  			<version>2.17</version>
  			<configuration>
                    <suiteXmlFiles>
                        <suiteXmlFile>src/test/java/testng.xml</suiteXmlFile>
                    </suiteXmlFiles>
                </configuration>
  		</plugin>
  	</plugins>
  </build>
</project>